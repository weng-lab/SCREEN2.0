{
  "name": "screen2.0",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write src",
    "export": "next export"
  },
  "lint-staged": {
    "linters": {
      "src": [
        "prettier --write",
        "git add"
      ]
    }
