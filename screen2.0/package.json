  "configureWebpack": {
    "resolve": {
      "extensions": [
        "*",
        ".mjs",
        ".js",
        ".jsx",
        ".json",
        ".gql",
        ".graphql",
        ".jpeg",
        ".jpg",
        ".png"
      ]
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.14",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.0",
    "@types/node": "^20.1.1",
    "@types/react": "^18.2.6",
    "@types/react-dom": "18.2.4",
    "@weng-lab/psychscreen-ui-components": "^0.7.0",
    "@weng-lab/ts-ztable": "^4.0.1",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "graphql": "^16.6.0",
    "next": "13.4.3",
    "normalize.css": "^8.0.1",
    "postcss": "8.4.24",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "file-loader": "^6.2.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "webpack": "^5.83.1",
    "webpack-cli": "^5.1.1"
  },
  "prettier": {
    "trailingComma": "es5",
    "printWidth": 140,
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSpacing": true,
    "singleQuote": false,
    "semi": false
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
